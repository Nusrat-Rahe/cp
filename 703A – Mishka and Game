#include <bits/stdc++.h>
using namespace std;

int main() {
    int n, a, b, mishka = 0, chris = 0;
    cin >> n;
    while(n--){
        cin >> a >> b;
        if(a > b) 
        mishka++;
        else if(b > a) 
        chris++;
    }
    if(mishka > chris) cout << "Mishka";
    else if(chris > mishka) cout << "Chris";
    else cout << "Friendship is magic!^^";
    return 0;
}

#include <iostream>
using namespace std;

int main() {
    string word;
    cin >> word;

    int countUpper = 0;
    int countLower = 0;

    // Count uppercase and lowercase letters
    for (int i = 0; i < word.length(); i++) {
        if (word[i] >= 'A' && word[i] <= 'Z') {
            countUpper++;
        } else if (word[i] >= 'a' && word[i] <= 'z') {
            countLower++;
        }
    }


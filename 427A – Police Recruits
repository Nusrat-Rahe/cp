#include <iostream>
using namespace std;

int main() {
    int n;
    cin >> n;

    int available = 0; 
    int untreated = 0; 

    for (int i = 0; i < n; i++) {
        int event;
        cin >> event;

        if (event > 0) {
            available += event;
        } else {
            if (available > 0) {
                available--;
            } else {
                untreated++;
            }
        }
    }

    cout << untreated;
    return 0;
}

#include <bits/stdc++.h>
using namespace std;

int main() {
    int n; 
    string s;
    cin >> n >> s;
    map<string,int> mp;

    for(int i = 0; i < n-1; i++)
        mp[s.substr(i,2)]++;

    string best = "";
    int mx = 0;

    for(auto &p : mp) 
        if(p.second > mx) best = p.first, mx = p.second;

    cout << best;
}

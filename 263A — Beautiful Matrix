#include <bits/stdc++.h>
using namespace std;

int main() {
    int a[5][5];
    int x, y;

    // Input নেওয়া
    for (int i = 0; i < 5; i++) {
        for (int j = 0; j < 5; j++) {
            cin >> a[i][j];
            if (a[i][j] == 1) {
                x = i;
                y = j;
            }
        }
    }

    // 1 কে (2,2)-এ আনতে move সংখ্যা
    cout << abs(x - 2) + abs(y - 2);
    return 0;
}
